<?xml version="1.0"?>
<PluginConfig>
	<Field type="textfield" id="username" >
		<Label>Tesla Motors Email:</Label>
	</Field>
	<Field type="textfield" id="password" secure="true">
		<Label>Tesla Motors Password:</Label>
	</Field>
	<Field type="textfield" id="mfacode" secure="true">
		<Label>(Optional) Tesla Motors MFA Passcode:</Label>
	</Field>
	<Field type="textfield" id="mfaid" secure="true">
		<Label>(Optional) Tesla Motors MFA ID:</Label>
	</Field>
	<Field id="showDebugInfo" type="checkbox">
		<Label>Enable debugging:</Label>
	</Field>
</PluginConfig>
